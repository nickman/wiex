<server>
	<mbean code="com.adp.sbs.metrics.tracing.collectors.CollectorService" name="com.adp.sbs.metrics:service=ConnectionTimeOut,name=TelDev">
		<constructor>
			<arg type="java.lang.String" value="com.adp.sbs.metrics.tracing.collectors.jdbc.JDBCCollector2"/>
		</constructor>
		<attribute name="SegmentPrefixElements">TeleNet,AP01,Status</attribute>
		<attribute name="ConnectionFactoryClassName">com.adp.sbs.metrics.tracing.collectors.jdbc.CheckedDriverManagerJDBCConnectionFactory</attribute>
		<attribute name="AvailabilitySegment">TelDev,Checks</attribute>
		<attribute name="AvailabilitySQL">select sysdate from dual</attribute>
		<attribute name="ConnectionFactoryProperties">
			  jdbc.driver=oracle.jdbc.driver.OracleDriver
			  jdbc.url=jdbc:oracle:thin:@PAR1UNIXDB02.nj.adp.com:1521:TELDEV
			  jdbc.user=PAYNET
			  jdbc.password=PAYNET
			  check.connect.timeout=10000
			  check.connect.tracing.segment=TelDev,Checks,Connections
	      </attribute>
	</mbean>
	<mbean code="com.adp.sbs.metrics.tracing.collectors.CollectorService" name="com.adp.sbs.metrics:service=ConnectionTimeOut,name=CatatonicSleep">
		<constructor>
			<arg type="java.lang.String" value="com.adp.sbs.metrics.tracing.collectors.jdbc.JDBCCollector2"/>
		</constructor>
		<attribute name="SegmentPrefixElements">TeleNet,AP01,Status</attribute>
		<attribute name="ConnectionFactoryClassName">com.adp.sbs.metrics.tracing.collectors.jdbc.CheckedDriverManagerJDBCConnectionFactory</attribute>
		<attribute name="AvailabilitySegment">CatatonicSleep,Checks</attribute>
		<attribute name="AvailabilitySQL">select sysdate from dual</attribute>
		<attribute name="ConnectionFactoryProperties">
			  jdbc.driver=com.adp.sbs.metrics.tracing.collectors.jdbc.CatatonicJDBCDriver
			  jdbc.url=jdbc:catatonic://sleep.a.long.time
			  jdbc.user=SLEEP
			  jdbc.password=SLEEP
			  check.connect.timeout=10000
			  check.connect.tracing.segment=CatatonicSleep,Checks,Connections
	      </attribute>
	</mbean>
	<mbean code="com.adp.sbs.metrics.tracing.collectors.CollectorService" name="com.adp.sbs.metrics:service=ConnectionTimeOut,name=CatatonicSocket">
		<constructor>
			<arg type="java.lang.String" value="com.adp.sbs.metrics.tracing.collectors.jdbc.JDBCCollector2"/>
		</constructor>
		<attribute name="SegmentPrefixElements">TeleNet,AP01,Status</attribute>
		<attribute name="ConnectionFactoryClassName">com.adp.sbs.metrics.tracing.collectors.jdbc.CheckedDriverManagerJDBCConnectionFactory</attribute>
		<attribute name="AvailabilitySegment">CatatonicSocket,Checks</attribute>
		<attribute name="AvailabilitySQL">select sysdate from dual</attribute>
		<attribute name="ConnectionFactoryProperties">
			  jdbc.driver=com.adp.sbs.metrics.tracing.collectors.jdbc.CatatonicJDBCSocketDriver
			  jdbc.url=jdbc:catatonic://sleep.a.long.time
			  jdbc.user=SLEEP
			  jdbc.password=SLEEP
			  check.connect.timeout=10000
			  check.connect.tracing.segment=CatatonicSocket,Checks,Connections
	      </attribute>
	</mbean>
	
</server>
